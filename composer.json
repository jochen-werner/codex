{
    "name": "codex/codex",
    "description": "Laravel 5 Documentation Platform",
    "keywords": ["codex", "documentation", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shea Lewis",
            "email": "shea.lewis89@gmail.com"
        },
        {
            "name": "Robin Radic",
            "email": "robin@radic.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5.9",
        "caffeinated/beverage": "~3.0",
        "illuminate/support": "5.1.*",
        "erusev/parsedown": "~1.5",
        "erusev/parsedown-extra": "~0.7",
        "symfony/yaml": "~2.7",
        "vierbergenlars/php-semver": "3.0.*"
    },
    "require-dev": {
        "caffeinated/dev": "dev-master@dev"
    },
    "autoload": {
        "psr-4": {
            "Codex\\Codex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codex\\Tests\\Codex\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
